{
    "paths": {
        "home": "/home/$USER",
        "config": "/home/$USER/.config/overtchat",
        "install": {
            "root": "/home/$USER",
            "service": "/home/$USER/Service-Overtchat",
            "server": "/home/$USER/Serveur-Overtchat",
            "tmp": "/tmp/.Overtchat"
        },
        "bin": {
            "service_lib": "/home/$USER/Service-Overtchat/bin/Lib"
        }
    },
    "git": {
        "repo": "https://github.com/servus033-cloud/Overtchat.git",
        "branch": "main"
    },
    "build": {
        "makefile": "/tmp/.Overtchat/Install/MAKEFILE",
        "target": "release"
    },
    "security": {
        "api_key": "44c63e560d1dab5208bb507c8126cbb5204e569b5b85db0adb67b8fbcaf5755b"
    },
    "commands": [
        {
            "command": "jq",
            "description": "Gestionnaire Json"
        },
        {
            "command": "msmtp",
            "description": "Gestionnaire msmtp"
        }
        {
            "command": "git",
            "description": "Gestionnaire Git"
        }
        {
            "command": "dig",
            "description": "Commande"
        }
        {
            "command": "mariadb",
            "description": "Gestionnaire MariaDB",
            "function": "mysqld"
        }
    ],
    "colors": {
        "reset": "\u001b[0m",
        "red": "\u001b[31m",
        "green": "\u001b[32m",
        "yellow": "\u001b[33m",
        "blue": "\u001b[34m",
        "magenta": "\u001b[35m",
        "cyan": "\u001b[36m",
        "white": "\u001b[37m"
    },
    "prompts": {
        "accept": "${green}Accept :${reset}",
        "refus": "${red}Refuser :${reset}",
        "info": "${blue}Informations :${reset}",
        "quest": "${yellow}Question :${reset}",
        "warn": "${yellow}Attention :${reset}"
    },
    "setup-base": [
        {
            "name": "Service-Overtchat",
            "install": 0,
        }
        {
            "name": "Serveur-Overtchat",
            "install": 0,
        }
        {
            "name": "Github",
            "load": 0,
        }
        {
            "name": "overtchat.json",
            "load": 0
        }
    ]
}